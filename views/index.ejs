<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="icon" type="image/x-icon" href="/ejs.svg">
    <link rel="stylesheet" href="/style.css">
  </head>
  <body>
    <h1>Inventory App</h1>

    <%- include('partials/link.ejs'); %>
    
    <div class="center">

      <div class="title-container">
      <ol>
          
      <% items.forEach(function(user){ %>
      <li>
      <h3><%= user.item_name %></h3>
      <a href="/view/<%= user.id %>">View</a>
      </li>
      <% }); %>
          
      </ol>
      </div>

    </div>
  </body>
</html>
